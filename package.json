{
	"name": "slavyanskaya",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt build",
		"migrate": "npm run delete-docs & nuxt generate & npm run create-folder-and-copy-docs & npm run copy-files",
		"copy-files": "Robocopy \"to copy\" docs",
		"create-folder-and-copy-docs": "mkdir docs & Robocopy dist docs /E",
		"delete-docs": "rmdir /s /q docs",
		"start": "nuxt start",
		"export": "nuxt export",
		"serve": "nuxt serve",
		"build:gh-pages": "SET DEPLOY_ENV='GH_PAGES' nuxt build --target static",
		"generate:gh-pages": "SET DEPLOY_ENV='GH_PAGES' && nuxt build --target static && nuxt export",
		"deploy": "gh-pages -d dist"
	},
	"dependencies": {
		"@nuxt/content": "^1.3.2",
		"@nuxtjs/axios": "^5.11.0",
		"@nuxtjs/sitemap": "^2.4.0",
