		"nuxt-sass-resources-loader": "^2.0.5",
		"save": "^2.4.0",
		"vue-frag": "^1.4.0",
		"vue-slick-carousel": "^1.0.6",
		"vueperslides": "^2.10.2"
	},
	"devDependencies": {
